{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "audio-limiter",
  "version": "0.1.0",
  "identifier": "com.absx.audio-limiter",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "width": 100,
        "height": 20,
        "decorations": false,
        "transparent": false,
        "resizable": false,
        "title": "音量控制",
        "visible": false
      }
    ],
    "security": {
      "csp": {}
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  },
  "plugins": {}
}
